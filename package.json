{
  "name": "npm-app",
  "version": "1.0.0",
  "description": "npm offline readme search",
  "main": "index.js",
  "scripts": {
    "start": "wzrd js/index.js:js/bundle.js -- -t ractivate",
    "server": "node server.js",
    "clone": "gasket run clone",
    "index": "gasket run index"
  },
  "author": "max ogden",
  "license": "BSD",
  "dependencies": {
    "classie": "^0.1.0",
    "corsify": "^2.0.0",
    "format-data": "^1.0.1",
    "marked": "^0.3.2",
    "ndjson": "^1.3.0",
    "ractive": "^0.6.1",
    "routes-router": "^4.1.1",
    "sqlite-search": "^3.2.0",
    "through2": "^0.6.3"
  },
  "devDependencies": {
    "dat": "^6.9.6",
    "gasket": "^2.0.0",
    "ractivate": "^0.2.0",
    "wzrd": "^1.2.1"
  },
  "gasket": {
    "clone": [
      "dat clone http://npm.dathub.org --skim npm-dat"
    ],
    "index": [
      "dat cat --path=npm-dat",
      "jsonfilter --match=\"this.name && this.readme && this.readme !== 'ERROR: No README.md file found!'\"",
      "node import.js"
    ]
  }
}
